package com.intelitune.nwms.service;

import java.util.List;

import com.intelitune.nwms.model.UnitState;

/**
 * 
 * @author Louis
 *
 */
public interface UnitStateService {

	/**
	 * 
	 * @return List<UnitState>
	 * 查询所有的UnitState
	 */
	List<UnitState> findAll();
	/**
	 * 
	 * @param String code
	 * @return UnitState
	 * 根据UnitState的code查询对应的UnitState
	 */
	UnitState getUnitState(String code);
	/**
	 * 
	 * @param UnitState unitState
	 * @return void
	 * 保存UnitState
	 */
	void saveUnitState(UnitState unitState);
	/**
	 * 
	 * @param UnitState unitState
	 * @return void
	 * 更新UnitState
	 */
	void updateUnitState(UnitState unitState);
	/**
	 * 
	 * @param String id
	 * @return UnitState
	 * 根据UnitState的Id查询对应的UnitState
	 */
	UnitState findUnitState(String id);
}

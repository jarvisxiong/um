<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.Material"
        table="intt_material"
    >

        <id
            name="id"
            column="id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Material.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="inttClientDetailWSId"
            type="java.lang.String"
            update="true"
            insert="true"
            column="inttClientDetailWSId"
        />

        <property
            name="value"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="value"
        />

        <set
            name="rates"
            table="intt_rates"
            lazy="true"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="materialId"
            >
            </key>

            <many-to-many
                class="com.intelitune.nwms.model.Rate"
                column="rateId"
                outer-join="auto"
             />

        </set>

        <many-to-one
            name="state"
            class="com.intelitune.nwms.model.MaterialState"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="state"
        />

        <property
            name="lastModifiedTime"
            type="java.sql.Timestamp"
            update="true"
            insert="true"
            column="lastModifiedTime"
        />

        <property
            name="creationTime"
            type="java.sql.Timestamp"
            update="true"
            insert="true"
            column="creationTime"
        />

        <list
            name="zones"
            table="intt_zones_material"
            lazy="true"
            cascade="none"
        >

            <key
                column="materialId"
            >
            </key>

            <index
                column="indexId"
            />

            <many-to-many
                class="com.intelitune.nwms.model.Zone"
                column="zoneId"
                outer-join="auto"
             />

        </list>

        <many-to-one
            name="shelflife"
            class="com.intelitune.nwms.model.Shelflife"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="shelflife"
        />

        <list
            name="unitPackages"
            lazy="true"
            inverse="true"
            cascade="none"
        >

            <key
                column="materialId"
            >
            </key>

            <index
                column="indexId"
            />

            <one-to-many
                  class="com.intelitune.nwms.model.UnitPackage"
            />

        </list>

        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            column="remark"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="description"
        />

        <property
            name="alias"
            type="java.lang.String"
            update="true"
            insert="true"
            column="alias"
        />

        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            column="code"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Material.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

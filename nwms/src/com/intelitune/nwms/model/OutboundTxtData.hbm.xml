<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.OutboundTxtData"
        table="intt_outboundTxtData"
    >

        <id
            name="id"
            column="id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-OutboundTxtData.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="opType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="opType"
        />

        <property
            name="orderJobId"
            type="java.lang.String"
            update="true"
            insert="true"
            column="orderJobId"
        />

        <property
            name="jobId"
            type="java.lang.String"
            update="true"
            insert="true"
            column="jobId"
        />

        <property
            name="binCode"
            type="java.lang.String"
            update="true"
            insert="true"
            column="binCode"
        />

        <property
            name="productCode"
            type="java.lang.String"
            update="true"
            insert="true"
            column="productCode"
        />

        <property
            name="SN"
            type="java.lang.String"
            update="true"
            insert="true"
            column="SN"
        />

        <property
            name="qty"
            type="java.lang.String"
            update="true"
            insert="true"
            column="qty"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-OutboundTxtData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

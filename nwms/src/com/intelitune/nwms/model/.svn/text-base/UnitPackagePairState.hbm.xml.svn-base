<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.UnitPackagePairState"
        table="intt_unitPackagePairState"
    >

        <id
            name="id"
            column="id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-UnitPackagePairState.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="description"
        />

        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            column="code"
        />
		 <set name="unitPackagePairs" inverse="true">
            <key>
                <column name="state"/>
            </key>
            <one-to-many class="com.intelitune.nwms.model.UnitPackagePair" />
        </set>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-UnitPackagePairState.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

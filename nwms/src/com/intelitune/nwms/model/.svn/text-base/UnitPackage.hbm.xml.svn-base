<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.UnitPackage"
        table="intt_unitPackage"
    >

        <id
            name="id"
            column="id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-UnitPackage.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="state"
            class="com.intelitune.nwms.model.UnitPackageState"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="state"
        />

        <property
            name="weight"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="weight"
        />

        <property
            name="length"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="length"
        />

        <property
            name="breadth"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="breadth"
        />

        <property
            name="height"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="height"
        />

        <property
            name="volume"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="volume"
        />

        <set
            name="unitPackagePairs"
            lazy="true"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="unitPackageId"
            >
            </key>

            <one-to-many
                  class="com.intelitune.nwms.model.UnitPackagePair"
            />

        </set>

        <many-to-one
            name="unit"
            class="com.intelitune.nwms.model.Unit"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="unit"
        />

        <many-to-one
            name="material"
            class="com.intelitune.nwms.model.Material"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="materialId"
        />

        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            column="remark"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="description"
        />

        <property
            name="alias"
            type="java.lang.String"
            update="true"
            insert="true"
            column="alias"
        />

        <property
            name="indexId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="indexId"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-UnitPackage.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

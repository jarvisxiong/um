<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.Shelflife"
        table="intt_shelflife"
    >

        <id
            name="id"
            column="id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Shelflife.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="denomination"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="denomination"
        />

        <many-to-one
            name="unit"
            class="com.intelitune.nwms.model.Unit"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="unit"
        />

        <many-to-one
            name="state"
            class="com.intelitune.nwms.model.ShelflifeState"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="state"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Shelflife.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

package com.intelitune.nwms.model;
/**
 * @hibernate.class
 * table = "intt_outboundTxtData"
 * @author chase
 *
 */
public class OutboundTxtData implements java.io.Serializable{
	
	public String id;
	
	public String jobId;
	
	public String binCode;
	
	public String productCode;
	
	public String sN;
	
	public String qty;
	
	public String orderJobId;
	
	public Integer opType;
	
	public int isCorrect;
	
	
	
	public static int  outbound=0;
	public static int  inbound=1;
	
	/**
	 * @hibernate.property
	 * @return
	 */
public Integer getOpType() {
		return opType;
	}

	public void setOpType(Integer opType) {
		this.opType = opType;
	}

/**
 * @hibernate.property
 * @return
 */
	public String getOrderJobId() {
		return orderJobId;
	}

	public void setOrderJobId(String orderJobId) {
		this.orderJobId = orderJobId;
	}

	/**
	 * @hibernate.id
 *   generator-class = "uuid"
	 * @return
	 */
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	/**
	 * @hibernate.property
	 * @return
	 */
	public String getJobId() {
		return jobId;
	}

	public void setJobId(String jobId) {
		this.jobId = jobId;
	}

	/**
	 * @hibernate.property
	 * @return
	 */
	public String getBinCode() {
		return binCode;
	}

	public void setBinCode(String binCode) {
		this.binCode = binCode;
	}

	/**
	 * @hibernate.property
	 * @return
	 */
	public String getProductCode() {
		return productCode;
	}

	public void setProductCode(String productCode) {
		this.productCode = productCode;
	}

	/**
	 * @hibernate.property
	 * @return
	 */
	public String getSN() {
		return sN;
	}

	public void setSN(String sn) {
		sN = sn;
	}

	/**
	 * @hibernate.property
	 * @return
	 */
	public String getQty() {
		return qty;
	}

	public void setQty(String qty) {
		this.qty = qty;
	}

	public int getIsCorrect() {
		return isCorrect;
	}

	public void setIsCorrect(int isCorrect) {
		this.isCorrect = isCorrect;
	}

	public static int getOutbound() {
		return outbound;
	}

	public static void setOutbound(int outbound) {
		OutboundTxtData.outbound = outbound;
	}

	public static int getInbound() {
		return inbound;
	}

	public static void setInbound(int inbound) {
		OutboundTxtData.inbound = inbound;
	}
	
	

}

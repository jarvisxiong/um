<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.Invoice"
        table="intt_invoice"
    >

        <id
            name="id"
            column="intt_id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Invoice.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="createTime"
            type="java.sql.Timestamp"
            update="true"
            insert="true"
            column="createTime"
        />
        
        <property
            name="so"
            type="java.lang.String"
            update="true"
            insert="true"
            column="so"
        />
        
         <property
            name="po"
            type="java.lang.String"
            update="true"
            insert="true"
            column="po"
        />

        <property
            name="lastTime"
            type="java.sql.Timestamp"
            update="true"
            insert="true"
            column="lastTime"
        />

        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            column="intt_code"
        />

        <property
            name="orderId"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="orderId"
        />

        <set
            name="items"
            lazy="true"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="invoice_id"
            >
            </key>

            <one-to-many
                  class="com.intelitune.nwms.model.Item"
            />

        </set>

        <many-to-one
            name="invoiveState"
            class="com.intelitune.nwms.model.InvoiceState"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="invoiveState"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Invoice.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

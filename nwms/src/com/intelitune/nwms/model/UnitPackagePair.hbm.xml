<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.intelitune.nwms.model.UnitPackagePair"
        table="intt_unitPackagePair"
    >

        <id
            name="id"
            column="id"
            type="java.lang.String"
        >
            <generator class="uuid">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-UnitPackagePair.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="state"
            class="com.intelitune.nwms.model.UnitPackagePairState"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"		
            column="state"
        />

        <property
            name="denomination"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="denomination"
        />

        <many-to-one
            name="otherUnitPackage"
            class="com.intelitune.nwms.model.UnitPackage"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="otherUnitPackage"
        />

        <many-to-one
            name="unitPackage"
            class="com.intelitune.nwms.model.UnitPackage"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="unitPackageId"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-UnitPackagePair.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

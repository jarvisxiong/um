
<body>

	<div class="box" />
		<div id="title">
			$title
		</div>

		$form.startTag()
		<TABLE>
			<TR>
				<TD>
					$form.getField("tf_customerName").label:
					$form.getField("tf_customerName")
				</TD>
				<TD align="left">
					<FONT color="red">$form.getField("l_msgRepeatedName").label</FONT>
				</TD>
			</TR>
			<TR>
				<TD>
					$form.getField("tf_customerAddress").label:
					$form.getField("tf_customerAddress")
				</TD>
			</TR>
			<TR>
				<TD>
					$form.getField("remark").label: $form.getField("remark")
				</TD>
			</TR>
			<TR>
				<TD align="center">
					$form.getField("submit").setStyle("border-width", "0px")
					$form.getField("submit").setStyle("cursor", "hand")
					$form.getField("submit").setStyle("background-image",
					"url($context/templates/white/img/buttonBack.gif)")
					$form.getField("submit").setStyle("background-position", "top")
					$form.getField("submit").setStyle("background-repeat", "repeat-x")
					$form.getField("submit").setStyle("margin", "0px")
					$form.getField("submit").setStyle("padding-left", "5px")
					$form.getField("submit").setStyle("padding-right", "5px")
					$form.getField("submit").setStyle("padding-left", "5px")
					$form.getField("submit").setStyle("height", "22px")
					$form.getField("submit").setStyle("font-family", "Verdana, Arial,
					Helvetica, sans-serif")
					$form.getField("submit").setStyle("font-size", "10pt")
					$form.getField("submit").setStyle("color", "#FFFFFF")

					$form.getField("reset").setStyle("border-width", "0px")
					$form.getField("reset").setStyle("cursor", "hand")
					$form.getField("reset").setStyle("background-image",
					"url($context/templates/white/img/buttonBack.gif)")
					$form.getField("reset").setStyle("background-position", "top")
					$form.getField("reset").setStyle("background-repeat", "repeat-x")
					$form.getField("reset").setStyle("margin", "0px")
					$form.getField("reset").setStyle("padding-left", "5px")
					$form.getField("reset").setStyle("padding-right", "5px")
					$form.getField("reset").setStyle("padding-left", "5px")
					$form.getField("reset").setStyle("height", "22px")
					$form.getField("reset").setStyle("font-family", "Verdana, Arial,
					Helvetica, sans-serif")
					$form.getField("reset").setStyle("font-size", "10pt")

					$form.getField("reset").setStyle("color", "#FFFFFF")
					$form.getField("submit") $form.getField("reset")
				</TD>
			</TR>
		</TABLE>
		$form.endTag()

		<DIV id="insert">
			<h3 align="center">
				$table
			</h3>
		</DIV>
</body>